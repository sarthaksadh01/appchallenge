    <!-- Contact -->
    <div id="contact" class="form">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <h2>Register</h2>
                    <ul class="list-unstyled li-space-lg">
                        <li class="address">Don't hesitate to give us a call or just use the contact form belo
                        <li><i class="fas fa-phone"></i><a class="blue" href="tel:8650605941">8650605941</a></li>
                        <li><i class="fas fa-phone"></i><a class="blue" href="tel:8076911425">8076911425</a></li>
                        <li><i class="fas fa-envelope"></i><a class="blue"
                                href="mailto:infox@gmail.com">infox@gmail.com</a></li>
                    </ul>
                </div> <!-- end of col -->
            </div> <!-- end of row -->
            <div class="row">
                <div class="col-lg-6 offset-lg-3">

                    <!-- Contact Form -->
                    <form id="contactForm" data-toggle="validator" data-focus="false" action="/register" method="POST">
                        <div class="form-group">
                            <input type="email" name="mainEmail" class="form-control-input" required>
                            <label class="label-control">Primary Email For Login</label>
                            <div class="help-block with-errors"></div>
                        </div>
                        <div class="form-group">
                            <input type="password" name="password" class="form-control-input" required>
                            <label class="label-control">Password</label>
                            <div class="help-block with-errors"></div>
                        </div>
                        <div class="form-group">
                            <input type="text" class="form-control-input" name="teamSize" required></textarea>
                            <label class="label-control">Team Size(Max 2)</label>
                            <div class="help-block with-errors"></div>
                        </div>
                        <div class="form-group">
                            <input type="tel" name="teamName" class="form-control-input" required>
                            <label class="label-control">Team Name</label>
                            <div class="help-block with-errors"></div>
                        </div>
                        <h4>Member 1 - Details</h4>
                        <div class="form-group">
                            <input type="text" name="username1" class="form-control-input" required>
                            <label class="label-control">Name</label>
                            <div class="help-block with-errors"></div>
                        </div>
                        <div class="form-group">
                            <input type="email" name="email1" class="form-control-input" required>
                            <label class="label-control">Email</label>
                            <div class="help-block with-errors"></div>
                        </div>
                        <div class="form-group">
                            <input type="text" name="contact1" class="form-control-input" required>
                            <label class="label-control">Mobile No.</label>
                            <div class="help-block with-errors"></div>
                        </div>
                    
                    <h4>Member 2 - Details</h4>
                    <div class="form-group">
                        <input type="text" name="username2" class="form-control-input">
                        <label class="label-control">Name</label>
                        <div class="help-block with-errors"></div>
                    </div>
                    <div class="form-group">
                        <input type="email" name="email2" class="form-control-input">
                        <label class="label-control">Email</label>
                        <div class="help-block with-errors"></div>
                    </div>
                    <div class="form-group">
                        <input type="text" name="contact2" class="form-control-input">
                        <label class="label-control">Mobile No.</label>
                        <div class="help-block with-errors"></div>
                    </div>
                
                        <div class="form-group checkbox">
                            <input type="checkbox" id="cterms" value="Agreed-to-Terms" required>I agree with all T&C
                            <div class="help-block with-errors"></div>
                        </div>
                        
                        <div class="form-group">
                            <button type="submit" class="form-control-submit-button">Register</button>
                        </div>
                    
                        <!-- <div class="form-message">
                            <div id="cmsgSubmit" class="h3 text-center hidden"></div>
                        </div> -->
                    </form>
                    <!-- end of contact form -->

                </div> <!-- end of col -->
            </div> <!-- end of row -->
        </div> <!-- end of container -->
    </div> <!-- end of form -->
    <!-- end of contact -->


